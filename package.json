{
  "name": "cliock",
  "version": "1.0.0",
  "description": "simple CLI for clockify",
  "main": "./bin/index.js",
  "scripts": {
    "build": "npx tsc",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|ts|json)\"",
    "dev": "npx ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "iok": "./bin/index.js"
  },
  "keywords": [
    "CLI",
    "clockify",
    "simple",
    "time",
    "productivity"
  ],
  "author": "Jared Gold",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
